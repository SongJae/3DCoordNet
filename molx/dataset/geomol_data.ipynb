   "source": [
    "from rdkit import Chem\n",
    "\n",
    "mol = Chem.MolFromSmiles(\"C=C1C(=O)C=NN1C.N\")\n",
    "for bond in mol.GetBonds():\n",
    "    bt = tuple(sorted([bond.GetBeginAtom().GetAtomicNum(), bond.GetEndAtom().GetAtomicNum()])), bond.GetBondTypeAsDouble()\n",
    "    print(\"bond type: \", bt)"
   ]
  }
